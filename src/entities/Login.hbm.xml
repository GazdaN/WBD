<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entities.Login" table="LOGIN" schema="NATALIAGAZDA">
        <id name="username">
            <column name="USERNAME" sql-type="varchar2(50)" length="50"/>
        </id>
        <property name="password">
            <column name="PASSWORD" sql-type="varchar2(50)" length="50" not-null="true"/>
        </property>
        <property name="uprawnienia">
            <column name="UPRAWNIENIA" sql-type="varchar2(50)" length="50" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>